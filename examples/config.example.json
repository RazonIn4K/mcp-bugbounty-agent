{
  "agent": {
    "name": "MCP Bug Bounty Research Agent",
    "version": "1.2.0",
    "premium": false,
    "use_docker": false
  },
  "apis": {
    "github_token": "ghp_your_token_here",
    "stripe_secret_key": "sk_test_your_key_here",
